<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ronifitgo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".ronifitgo.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RonifitGo">
        <activity
            android:name=".ronifitgo.Activity_splash"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ronifitgo.Activity_enter"
            android:exported="true" />

        <activity
            android:name=".ronifitgo.Activity_sign_up"
            android:exported="true" />

        <activity
            android:name=".ronifitgo.MainActivity"
            android:exported="true"/>

        <activity
            android:name=".ronifitgo.Activity_addMeasures"
            android:exported="true"/>

        <activity
            android:name=".ronifitgo.Activity_addWeight"
            android:exported="true"/>



    </application>

</manifest>